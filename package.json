{
  "name": "pwa-maps",
  "version": "0.1.0",
  "description": "A Progressive Web App Web Map",
  "scripts": {
    "present": "node node_modules/reveal-md/bin/reveal-md.js README.md",
    "start": "node node_modules/sw-precache/cli.js --root=public && http-server -p 80",
    "deploy": "netlify deploy --prod",
    "cache": "node node_modules/sw-precache/cli.js --root=public"
  },
  "keywords": [
    "progressive web app",
    "maps",
    "mapbox",
    "openmaptiles",
    "vector tiles",
    "getbounds"
  ],
  "author": "Malcolm Meyer",
  "license": "MIT",
  "dependencies": {
    "http-server": "^0.11.1",
    "sw-precache": "^5.2.1"
  },
  "devDependencies": {
    "reveal-md": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reyemtm/pwa-maps.git"
  },
  "bugs": {
    "url": "https://github.com/reyemtm/pwa-maps/issues"
  },
  "homepage": "https://github.com/reyemtm/pwa-maps#readme"
}
